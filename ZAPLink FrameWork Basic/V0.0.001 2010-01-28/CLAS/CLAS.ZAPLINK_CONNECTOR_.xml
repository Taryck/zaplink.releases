     <ITEM SCONAME="SOURCE" VERSION="1" CMPTYPE="1" EDITORDER="1 " AUTHOR="TBENSIAL" CREATEDON="20091118" PARPASSTYP="1" TYPTYPE="1" TYPE="TD_ABAPSOURCE">
      <TEXTS>
       <ITEM LANGU="E"/>
      </TEXTS>
     </ITEM>
    </PARAMETERS>
    <SOURCE>SPLIT source AT line_separator INTO TABLE result.</SOURCE>
