    <SOURCE>  DATA o_file TYPE to_file.
  DATA _fn    TYPE td_filename.
  DATA cx_file TYPE REF TO zaplink_cx_file.
  DATA cx_cont TYPE REF TO zaplink_cx_container.

  TRY.
      CREATE OBJECT o_file.
      o_file-&gt;set_filetype( zaplink_file=&gt;c_ft_local ).
      _fn = file.
      IF _fn IS INITIAL.
        CALL METHOD o_file-&gt;save_dialog
          EXPORTING
            filename = _fn.
      ENDIF.
      o_file-&gt;set_filename( _fn ).
    CATCH zaplink_cx_file INTO cx_file.
      CREATE OBJECT o_exception
        EXPORTING
          textid  = zaplink_cx_gui=&gt;system_error
          prev_cx = cx_file.
      o_exception-&gt;update( ).
      RAISE EXCEPTION o_exception.
  ENDTRY.

  TRY.
      container-&gt;save_to_file( o_file ).
    CATCH zaplink_cx_container INTO cx_cont.
      CREATE OBJECT o_exception
        EXPORTING
          textid  = zaplink_cx_gui=&gt;system_error
          prev_cx = cx_file.
      o_exception-&gt;update( ).
      RAISE EXCEPTION o_exception.
  ENDTRY.</SOURCE>
